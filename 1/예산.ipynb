   "source": [
    "def solution(d, budget):\n",
    "    answer = 0\n",
    "    \n",
    "    d = sorted(d)\n",
    "    for v in d:\n",
    "        if budget >= v:\n",
    "            budget -= v\n",
    "            answer += 1\n",
    "        else:\n",
    "            break\n",
    "    \n",
    "    return answer"
   ]
