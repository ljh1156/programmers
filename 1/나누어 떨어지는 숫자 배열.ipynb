   "source": [
    "def solution(arr, divisor):\n",
    "    answer = []\n",
    "    \n",
    "    for a in arr:\n",
    "        if a % divisor == 0:\n",
    "            answer.append(a)\n",
    "    if answer == []:\n",
    "        return [-1]\n",
    "    return sorted(answer)\n",
    "    \n"
   ]
