   "source": [
    "def solution(numbers):\n",
    "    answer = set([])\n",
    "    for i in range(0, len(numbers) - 1):\n",
    "        for j in range(i + 1, len(numbers)):\n",
    "            answer.add(numbers[i] + numbers[j])\n",
    "    answer = sorted(list(answer))\n",
    "            \n",
    "    return answer"
   ]
