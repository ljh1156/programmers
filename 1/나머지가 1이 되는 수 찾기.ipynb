    "def solution(n):\n",
    "    answer = 0\n",
    "    \n",
    "    if n % 2 == 0:\n",
    "        x = 3\n",
    "    else:\n",
    "        x = 2\n",
    "    while(1):\n",
    "        if n % x == 1:\n",
    "            return x\n",
    "        x += 2\n",
    "    \n",
    "    return answer"
