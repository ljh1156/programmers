   "source": [
    "def solution(absolutes, signs):\n",
    "    answer = 0\n",
    "    \n",
    "    for ab, si in zip(absolutes, signs):\n",
    "        if si:\n",
    "            answer += ab\n",
    "        else:\n",
    "            answer -= ab\n",
    "    \n",
    "    return answer"
   ]
