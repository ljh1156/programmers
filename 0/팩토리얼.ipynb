    "def solution(n):\n",
    "    answer = 1\n",
    "    \n",
    "    i = 2\n",
    "    while(1):\n",
    "        if i * answer > n:\n",
    "            return (i - 1)\n",
    "        else:\n",
    "            answer *= i\n",
    "        i+=1\n",
    "    \n",
    "    return answer"
   ]
