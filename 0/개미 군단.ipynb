    "def solution(hp):\n",
    "    answer = 0\n",
    "    \n",
    "    a = int(hp/5)\n",
    "    #\n",
    "    answer += a\n",
    "    hp = hp - a*5\n",
    "    b = int(hp/3)\n",
    "    answer += b\n",
    "    hp = hp - b*3\n",
    "    answer += hp\n",
    "    \n",
    "    \n",
    "    \n",
    "    return answer"
   ]
