   "source": [
    "def solution(s):\n",
    "    answer = 0\n",
    "    \n",
    "    n = 0\n",
    "    for c in s.split(' '):\n",
    "        if c != \"Z\":\n",
    "            answer += n\n",
    "            n = int(c)\n",
    "        else:\n",
    "            n = 0\n",
    "    answer += n\n",
    "    \n",
    "    return answer"
   ]
