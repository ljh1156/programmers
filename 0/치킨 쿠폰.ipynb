   "source": [
    "def solution(chicken):\n",
    "    answer = -1\n",
    "    if chicken >= 10:\n",
    "        answer = 1\n",
    "        chicken -= 10\n",
    "        answer += int(chicken / 9)\n",
    "    else:\n",
    "        return 0\n",
    "    return answer"
   ]
