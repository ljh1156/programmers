   "source": [
    "def solution(n):\n",
    "    answer = []\n",
    "    \n",
    "    i = 1\n",
    "    while(1):\n",
    "        \n",
    "        if n % i == 0:\n",
    "            answer.append(int(n/i))\n",
    "        if i == n:\n",
    "            break\n",
    "        i += 1\n",
    "        \n",
    "            \n",
    "    return answer[::-1]\n",
    "        \n",
    "    #return answer"
   ]
