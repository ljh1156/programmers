    "def solution(my_string):\n",
    "    answer = ''\n",
    "    \n",
    "    for c in list(my_string):\n",
    "        if c.isupper():\n",
    "            answer += c.lower()\n",
    "        else:\n",
    "            answer += c.upper()\n",
    "    \n",
    "    return answer"
   ]
